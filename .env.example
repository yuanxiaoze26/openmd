# 数据库类型 (mysql 或 sqlite)
DB_TYPE=mysql

# MySQL 数据库配置
DB_HOST=your-mysql-host
DB_PORT=3306
DB_NAME=openmd
DB_USER=your-mysql-user
DB_PASSWORD=your-mysql-password

# Session 密钥（用于加密用户会话，请使用 openssl rand -hex 32 生成）
SESSION_SECRET=your-random-secret-key-here

# Node.js 配置
PORT=3000
NODE_ENV=production
